{
  "name": "@bunbox/tree",
  "version": "0.1.1",
  "description": "Tree data structure implementation for Bunbox",
  "author": {
    "name": "Renato Rodrigues",
    "email": "renato@vulppi.dev"
  },
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/vulppi-dev/bunbox",
    "directory": "libs/tree"
  },
  "keywords": [
    "bunbox",
    "tree",
    "tree-structure",
    "scene-graph",
    "hierarchy",
    "parent-child",
    "acyclic",
    "reparenting",
    "event-emitter",
    "event-driven",
    "traversal",
    "visitor-pattern",
    "reactive",
    "dirty-state",
    "data-structures",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch --noEmit true --emitDeclarationOnly false",
    "build": "bun build:bundle && bun build:types",
    "build:bundle": "bun build src/index.ts --outdir dist --format esm",
    "build:types": "tsc --project tsconfig.json",
    "pub": "npm publish --access=public --tag=latest",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@bunbox/utils": "^0.0.2",
    "ulid": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.0"
  }
}
