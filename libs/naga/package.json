{
  "name": "@bunbox/naga",
  "version": "0.1.0",
  "description": "Simple WGSL parser for TypeScript",
  "author": {
    "name": "Renato Rodrigues",
    "email": "renato@vulppi.dev"
  },
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/vulppi-dev/bunbox",
    "directory": "libs/naga"
  },
  "keywords": [
    "naga",
    "wgsl",
    "spir-v"
  ],
  "type": "module",
  "main": "./dist/naga_wasm.js",
  "types": "./dist/naga_wasm.d.ts",
  "exports": {
    ".": {
      "types": "./dist/naga_wasm.d.ts",
      "import": "./dist/naga_wasm.js",
      "default": "./dist/naga_wasm.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pwsh -File build.ps1",
    "build:sh": "sh build.sh",
    "build:web": "pwsh -Command \"docker build -t naga-wasm-builder . && docker run --rm -v ${PWD}/rust:/workspace -v ${PWD}/dist:/output naga-wasm-builder build --target web --out-dir /output\"",
    "pub": "npm publish --access=public --tag=latest",
    "format": "prettier --write ."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.10.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.0"
  }
}
