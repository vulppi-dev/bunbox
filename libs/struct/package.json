{
  "name": "@bunbox/struct",
  "version": "2.0.1",
  "description": "TypeScript library for defining and working with C-like structs in memory, with support for alignment, nested structs, enums, arrays, and FFI/WebAssembly interop.",
  "author": {
    "name": "Renato Rodrigues",
    "email": "renato@vulppi.dev"
  },
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/vulppi-dev/bunbox",
    "directory": "libs/struct"
  },
  "keywords": [
    "bunbox",
    "struct",
    "binary",
    "buffer",
    "typescript",
    "serialization",
    "ffi",
    "wasm"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch --noEmit true --emitDeclarationOnly false",
    "build": "bun build:bundle && bun build:types",
    "build:bundle": "bun build src/index.ts --outdir dist --format esm",
    "build:types": "tsc --project tsconfig.json",
    "pub": "npm publish --access=public --tag=latest",
    "pub:tag": "npm publish --access=public",
    "format": "prettier --write ."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.0"
  }
}
