{
  "name": "@bunbox/utils",
  "version": "0.0.4",
  "description": "Utility functions for Bunbox",
  "author": {
    "name": "Renato Rodrigues",
    "email": "renato@vulppi.dev"
  },
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/vulppi-dev/bunbox",
    "directory": "libs/utils"
  },
  "keywords": [
    "bunbox",
    "utils",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch --noEmit true --emitDeclarationOnly false",
    "build": "bun build:bundle && bun build:types",
    "build:bundle": "bun build src/index.ts --outdir dist --format esm",
    "build:types": "tsc --project tsconfig.json",
    "pub": "npm publish --access=public --tag=latest",
    "format": "prettier --write ."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.9.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.0"
  }
}
